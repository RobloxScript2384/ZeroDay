local KGHJDHDKS = {}

function KGHJDHDKS:CreateWindow(TITLE,VERSION)
	
	-- Instances:

	local ZeroDay = Instance.new("ScreenGui")
	local Main = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local TopFrame = Instance.new("Frame")
	local UICorner_2 = Instance.new("UICorner")
	local BottomFramePiece = Instance.new("Frame")
	local Title = Instance.new("TextLabel")
	local VER = Instance.new("TextLabel")
	local TabButtons = Instance.new("Frame")
	local UICorner_3 = Instance.new("UICorner")
	local Buttons = Instance.new("ScrollingFrame")
	local UIListLayout = Instance.new("UIListLayout")
	local Tabs = Instance.new("Frame")

	--Properties:

	ZeroDay.Name = "ZeroDay"
	ZeroDay.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	ZeroDay.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	Main.Name = "Main"
	Main.Parent = ZeroDay
	Main.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
	Main.BorderSizePixel = 0
	Main.ClipsDescendants = true
	Main.Position = UDim2.new(0.287234038, 0, 0.244585991, 0)
	Main.Size = UDim2.new(0, 600, 0, 400)

	UICorner.Parent = Main

	TopFrame.Name = "TopFrame"
	TopFrame.Parent = Main
	TopFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	TopFrame.BorderSizePixel = 0
	TopFrame.Size = UDim2.new(0, 600, 0, 77)

	UICorner_2.Parent = TopFrame

	BottomFramePiece.Name = "BottomFramePiece"
	BottomFramePiece.Parent = TopFrame
	BottomFramePiece.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	BottomFramePiece.BorderSizePixel = 0
	BottomFramePiece.Position = UDim2.new(0, 0, 0.896103919, 0)
	BottomFramePiece.Size = UDim2.new(0, 600, 0, 19)

	Title.Name = "Title"
	Title.Parent = TopFrame
	Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Title.BackgroundTransparency = 1.000
	Title.BorderColor3 = Color3.fromRGB(27, 42, 53)
	Title.BorderSizePixel = 0
	Title.Position = UDim2.new(0.0183333345, 0, 0.16883117, 0)
	Title.Size = UDim2.new(0, 280, 0, 50)
	Title.Font = Enum.Font.SourceSans
	Title.Text = TITLE
	Title.TextColor3 = Color3.fromRGB(255, 255, 255)
	Title.TextScaled = true
	Title.TextSize = 14.000
	Title.TextWrapped = true
	Title.TextXAlignment = Enum.TextXAlignment.Left

	VER.Name = "Version"
	VER.Parent = TopFrame
	VER.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	VER.BackgroundTransparency = 1.000
	VER.BorderColor3 = Color3.fromRGB(27, 42, 53)
	VER.BorderSizePixel = 0
	VER.Position = UDim2.new(0.0250000004, 0, 0.713999987, 0)
	VER.Size = UDim2.new(0, 78, 0, 22)
	VER.Font = Enum.Font.SourceSans
	VER.Text = VERSION
	VER.TextColor3 = Color3.fromRGB(255, 255, 255)
	VER.TextScaled = true
	VER.TextSize = 14.000
	VER.TextWrapped = true
	VER.TextXAlignment = Enum.TextXAlignment.Left

	TabButtons.Name = "TabButtons"
	TabButtons.Parent = Main
	TabButtons.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	TabButtons.BorderSizePixel = 0
	TabButtons.Position = UDim2.new(0, 0, 0.192499995, 0)
	TabButtons.Size = UDim2.new(0, 160, 0, 323)

	UICorner_3.Parent = TabButtons

	Buttons.Name = "Buttons"
	Buttons.Parent = TabButtons
	Buttons.Active = true
	Buttons.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Buttons.BackgroundTransparency = 1.000
	Buttons.BorderSizePixel = 0
	Buttons.Position = UDim2.new(0.0250000004, 0, 0.0340557285, 0)
	Buttons.Size = UDim2.new(0, 149, 0, 306)
	Buttons.CanvasSize = UDim2.new(0, 0, 0, 0)
	Buttons.ScrollBarThickness = 0

	UIListLayout.Parent = Buttons
	UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout.Padding = UDim.new(0.00999999978, 0)

	Tabs.Name = "Tabs"
	Tabs.Parent = Main
	Tabs.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Tabs.BackgroundTransparency = 1.000
	Tabs.BorderSizePixel = 0
	Tabs.Position = UDim2.new(0.266666681, 0, 0.219999999, 0)
	Tabs.Size = UDim2.new(0, 440, 0, 312)
		
	local tabs = {}
	
	function tabs:CreateTab(TITLE)	
		
		local TabButtonTemplate = Instance.new("TextButton")
		local UICorner_4 = Instance.new("UICorner")
		local TabTemplate = Instance.new("ScrollingFrame")
		local UIListLayout_2 = Instance.new("UIListLayout")

		TabButtonTemplate.Name = TITLE
		TabButtonTemplate.Parent = Buttons
		TabButtonTemplate.BackgroundColor3 = Color3.fromRGB(44, 171, 255)
		TabButtonTemplate.BackgroundTransparency = 1.000
		TabButtonTemplate.BorderSizePixel = 0
		TabButtonTemplate.Size = UDim2.new(0, 141, 0, 39)
		TabButtonTemplate.Font = Enum.Font.SourceSans
		TabButtonTemplate.Text = TITLE
		TabButtonTemplate.TextColor3 = Color3.fromRGB(255, 255, 255)
		TabButtonTemplate.TextScaled = true
		TabButtonTemplate.TextSize = 14.000
		TabButtonTemplate.TextWrapped = true

		UICorner_4.Parent = TabButtonTemplate

		TabTemplate.Name = "TabTemplate"
		TabTemplate.Parent = Tabs
		TabTemplate.Active = true
		TabTemplate.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TabTemplate.BackgroundTransparency = 1.000
		TabTemplate.BorderSizePixel = 0
		TabTemplate.Position = UDim2.new(0.0181818176, 0, 0.022435898, 0)
		TabTemplate.Size = UDim2.new(0, 426, 0, 299)
		TabTemplate.ScrollBarThickness = 0
		TabTemplate.Visible = false

		UIListLayout_2.Parent = TabTemplate
		UIListLayout_2.HorizontalAlignment = Enum.HorizontalAlignment.Center
		UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout_2.Padding = UDim.new(0.0199999996, 0)
		
		Buttons.CanvasSize = UDim2.new(0, UIListLayout.AbsoluteContentSize.X, 0, UIListLayout.AbsoluteContentSize.Y)
		
	end
	
	return tabs
end

return KGHJDHDKS
